    "babel-preset-stage-0": "^6.24.1",
    "date-time-picker-react": "^0.0.7",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.58.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "moment": "^2.19.2",
    "prettier": "^1.7.4",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "webpack": "^3.8.1"
